OBJECT.SHAPE BoyR3Bob.id,BoyR3Bob$


dir=boyright

OBJECT.X Boy(dir,count),x
OBJECT.Y Boy(dir,count),y
OBJECT.SHAPE ON, Boy(dir,count)

WHILE Play=1
    dir=0
    k$=inkey$
    IF k$<>"" THEN dir=asc(k$)
    IF k$ = "s" OR dir=30 THEN x=x+4:GOSUB goright
    IF k$ = "a" OR dir=31 THEN x=x-4:GOSUB goleft
    IF k$ = "w" OR dir=28 THEN y=y-4:GOSUB goup
    IF k$ = "z" OR dir=29 THEN y=y+4:GOSUB godown
    IF k$ = "q" THEN SYSTEM
    OBJECT.X Boy(dir,count),x
    OBJECT.Y Boy(dir,count),y
    
WEND

godown:
OBJECT.OFF Boy(dir,count)
count=count+1
IF count > 2 THEN count=0
dir=boydown
OBJECT.ON Boy(dir,count)
RETURN 

goleft:
OBJECT.OFF Boy(dir,count)
count=count+1
IF count > 2 THEN count=0
dir=boyleft
